{
  "name": "@fett/synology-api",
  "version": "0.0.1-beta.10",
  "description": "synology api for nodejs",
  "module": "./lib/esm/index.js",
  "main": "./lib/cjs/index.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js"
    },
    "./browser": {
      "import": "./lib/esm/core.js",
      "types": "./lib/types/core.d.ts"
    }
  },
  "bin": {
    "synology": "./bin/syno",
    "syno": "./bin/syno"
  },
  "files": [
    "lib",
    "bin"
  ],
  "author": "ChrisSong1994 <https://github.com/ChrisSong1994>",
  "keywords": [
    "synology",
    "api",
    "nodejs",
    "browser",
    "typescript",
    "esmodule",
    "cjs",
    "commonjs"
  ],
  "scripts": {
    "dev:esm": "tsc --sourceMap --watch && tsc-alias",
    "dev:cjs": "tsup --config tsup.config.ts --watch --env.NODE_ENV development",
    "build:cjs": "tsup --config tsup.config.ts",
    "build:esm": "tsc  && tsc-alias",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "docs:dev": "rspress dev",
    "docs:build": "rspress build",
    "docs:preview": "rspress preview",
    "clean": "rimraf lib",
    "lint": "eslint src --ext .ts --fix",
    "prettier": "prettier --write 'src/**/*.ts'",
    "format": "npm run prettier && npm run lint",
    "prepublish": "npm run build",
    "pub": "npm publish --access=public",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "license": "ISC",
  "packageManager": "pnpm@10.8.1",
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "cross-env": "^7.0.3",
    "eslint": "9.25.1",
    "execa": "^9.6.0",
    "globals": "^16.1.0",
    "prettier": "3.5.3",
    "rimraf": "^6.0.1",
    "rspress": "^1.44.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsup": "^8.5.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "fs-extra": "^11.3.0",
    "ky": "^1.8.1",
    "ora": "^8.2.0"
  }
}
