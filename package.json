{
  "name": "@fett/synology-api",
  "version": "0.1.3",
  "description": "synology api for nodejs",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": {
    "synology": "./lib/cli/index.js",
    "syno": "./lib/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "react-native": "./lib/index.rn.js"
    },
    "./react-native": {
      "import": "./lib/index.rn.js",
      "require": "./lib/index.rn.js"
    }
  },
  "files": [
    "lib",
    "bin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "author": "ChrisSong1994 <https://github.com/ChrisSong1994>",
  "repository": {
    "type": "git",
    "url": "git@github.com:ChrisSong1994/synology-api.git"
  },
  "homepage": "https://chrissong1994.github.io/synology-api",
  "keywords": [
    "synology",
    "api",
    "nodejs",
    "browser",
    "typescript",
    "esmodule",
    "cli",
    "vitest"
  ],
  "scripts": {
    "dev": "npm run clean && concurrently --kill-others \"tsc --sourceMap -w\" \"tsc-alias -w\"",
    "build": "npm run clean && tsc  && tsc-alias",
    "build:dev": "cross-env NODE_ENV=development npm run build",
    "docs:dev": "rspress dev",
    "docs:build": "cross-env BASE_PATH=/synology-api/ rspress build",
    "docs:preview": "rspress preview",
    "clean": "rimraf lib",
    "lint": "eslint src --ext .ts --fix",
    "prettier": "prettier --write 'src/**/*.ts'",
    "format": "npm run prettier && npm run lint",
    "prepublish": "npm run build",
    "pub": "npm publish --access=public",
    "pub:beta": "npm publish --access=public --tag=beta",
    "test": "vitest",
    "test:debug": "node --inspect-brk=9230 ./node_modules/vitest/vitest.mjs run --pool=forks",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@vitest/coverage-v8": "3.2.2",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "eslint": "9.25.1",
    "execa": "^9.6.0",
    "globals": "^16.1.0",
    "jsdom": "^26.1.0",
    "prettier": "3.5.3",
    "rimraf": "^6.0.1",
    "rspress": "^1.44.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "form-data": "^4.0.4",
    "fs-extra": "^11.3.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "ora": "^8.2.0"
  },
  "license": "MIT"
}
